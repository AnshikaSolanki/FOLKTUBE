extends layouts/main 
include mixins/videoBlock

block content 
    .search__header 
        h3 Searching for : #{searchingby}
    .search__videos 
        if videos.length === 0
            h5 No videos found!
        each item in videos
            +videoBlock({
                   title: item.title,
                   views:item.views,
                   videofile: item.videofile,
                   id: item.id
               })